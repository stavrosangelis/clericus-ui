{
  "name": "clericus-ui",
  "version": "1.2.0",
  "description": "This is the Clericus public user interface",
  "author": "Stavros Angelis",
  "license": "ISC",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "NODE_ENV=production react-app-rewired start",
    "dev": "NODE_ENV=development react-app-rewired start",
    "build:dev": "REACT_APP_ENV=prod.dev DOMAIN=http://devel.clericus.ie REACT_APP_VERSION=$npm_package_version REACT_APP_DOMAIN=http://devel.clericus.ie REACT_APP_APIPATH=http://devel.clericus.ie/api/ BUILD_PATH='./build-dev' react-app-rewired build",
    "build:devserver": "REACT_APP_ENV=prod.dev DOMAIN=http://devel.clericus.ie REACT_APP_VERSION=$npm_package_version REACT_APP_DOMAIN=http://devel.clericus.ie REACT_APP_APIPATH=http://devel.clericus.ie/api/ react-app-rewired build",
    "build": "react-app-rewired build",
    "test": "REACT_APP_APIPATH=http://localhost:5100/api/ react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^14.1.1",
    "axios": "^0.27.2",
    "axios-progress-bar": "^1.2.0",
    "bootstrap": "^5.1.3",
    "d3": "^7.4.4",
    "dompurify": "^2.3.6",
    "leaflet": "^1.8.0",
    "leaflet.heat": "^0.2.0",
    "moment": "^2.29.3",
    "pixi-viewport": "^4.34.4",
    "pixi.js": "^6.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.0",
    "react-datepicker": "^4.7.0",
    "react-dom": "^17.0.0",
    "react-leaflet": "^3.2.5",
    "react-leaflet-markercluster": "^3.0.0-rc1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "reactstrap": "^9.0.2",
    "recaptcha-v3": "^1.10.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.52.1"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "msw": "^0.39.2",
    "prettier": "^2.6.2",
    "react-app-rewired": "^2.2.1",
    "worker-loader": "^3.0.8"
  }
}
